{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.7",
        "graham-campbell/flysystem": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/mail": "^5.6",
        "illuminate/redis": "^5.6",
        "intervention/image": "^2.3",
        "laravel/lumen-framework": "5.6.*",
        "milon/barcode": "^5.3",
        "overtrue/laravel-pinyin": "^3.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "pdepend/pdepend": "^2.5",
        "phing/phing": "^2.16",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~7.0",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
